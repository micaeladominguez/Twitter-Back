datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
model User {
  id    String  @id @default(uuid())
  email String  @unique
  name  String
  password String
  isActive Boolean @default(true)
  phone String?
  createdAt DateTime @default(now())
}
